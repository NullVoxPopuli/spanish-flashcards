  "private": true,
  "description": "Small description for spanish-flashcards goes here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "type": "module",
  "imports": {
    "#app/*": "./app/*",
    "#config": "./app/config/environment",
    "#components/*": "./app/components/*"
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  },
  "scripts": {
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm format",
    "lint:format": "prettier . --cache --check",
    "build": "vite build",
    "dev": "vite",
    "lint:types": "ember-tsc --noEmit",
    "start": "vite"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "@babel/runtime": "^7.28.4",
    "@ember/app-tsconfig": "^2.0.0",
    "@embroider/core": "^4.2.4",
    "@embroider/legacy-inspector-support": "^0.1.3",
    "@embroider/macros": "^1.19.1",
    "@embroider/router": "^3.0.4",
    "@embroider/vite": "^1.3.2",
    "@glimmer/component": "^2.0.0",
    "@glint/ember-tsc": "^1.0.4",
    "@glint/template": "^1.6.2",
    "@glint/tsserver-plugin": "^2.0.4",
    "@rollup/plugin-babel": "^6.0.4",
    "@types/qunit": "^2.19.13",
    "@types/rsvp": "^4.0.9",
    "babel-plugin-ember-template-compilation": "^3.0.1",
    "decorator-transforms": "^2.3.0",
    "ember-source": "~6.10.0-alpha.1",
    "prettier": "^3.7.4",
    "prettier-plugin-ember-template-tag": "^2.1.2",
    "typescript": "^5.9.3",
    "vite": "^7.1.9"
  },
  "engines": {
    "node": ">= 24"
  },
  "ember": {
    "edition": "octane"
  },
  "dependencies": {
    "ember-scoped-css": "^2.0.4",
    "ember-strict-application-resolver": "^0.1.1",
    "ember-truth-helpers": "^5.0.0"
  }
